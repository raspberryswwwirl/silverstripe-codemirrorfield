{
    "name": "raspberryswwwirl/silverstripe-codemirrorfield",
    "description": "Proof-of-concept for a codemirror field component built with Vue.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "RaspberrySwwwirl",
            "email": "hello@raspberryswwwirl.com"
        }
    ],
    "require": {
        "silverstripe/framework": "~4.0",
        "silverstripe/cms": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "RS\\": "src/RS"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },    
    "minimum-stability": "dev",
    "prefer-stable": true
}
